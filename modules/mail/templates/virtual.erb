<%
domains = {}
emails = {}
@users.each do |user, data|
  data.fetch('email', []).each do |address|
    domain = address.split('@', 2).last
    domains[domain] = 'x'
    emails[address] = user
  end
end
domains.sort.each do |key, value|
  puts key, value
end
emails.sort.each do |key, value|
  puts key, value
end
%>
